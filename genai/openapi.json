{
  "components": {
    "schemas": {
      "ContributionMetadata": {
        "description": "Metadata about a contribution to be fetched.",
        "properties": {
          "id": {
            "title": "Id",
            "type": "string"
          },
          "selected": {
            "title": "Selected",
            "type": "boolean"
          },
          "type": {
            "$ref": "#/components/schemas/ContributionType"
          }
        },
        "required": ["type", "id", "selected"],
        "title": "ContributionMetadata",
        "type": "object"
      },
      "ContributionsIngestRequest": {
        "description": "Request to ingest contributions for a user's week (metadata only).",
        "properties": {
          "contributions": {
            "items": {
              "$ref": "#/components/schemas/ContributionMetadata"
            },
            "title": "Contributions",
            "type": "array"
          },
          "github_pat": {
            "title": "Github Pat",
            "type": "string"
          },
          "repository": {
            "title": "Repository",
            "type": "string"
          },
          "user": {
            "title": "User",
            "type": "string"
          },
          "week": {
            "title": "Week",
            "type": "string"
          }
        },
        "required": ["user", "week", "repository", "contributions", "github_pat"],
        "title": "ContributionsIngestRequest",
        "type": "object"
      },
      "ContributionType": {
        "description": "Types of GitHub contributions supported by the system.",
        "enum": ["commit", "pull_request", "issue", "release"],
        "title": "ContributionType",
        "type": "string"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "IngestTaskResponse": {
        "description": "Response from starting a contributions ingestion task.",
        "properties": {
          "created_at": {
            "format": "date-time",
            "title": "Created At",
            "type": "string"
          },
          "repository": {
            "title": "Repository",
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/TaskStatus",
            "default": "queued"
          },
          "summary_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Summary Id"
          },
          "task_id": {
            "title": "Task Id",
            "type": "string"
          },
          "total_contributions": {
            "title": "Total Contributions",
            "type": "integer"
          },
          "user": {
            "title": "User",
            "type": "string"
          },
          "week": {
            "title": "Week",
            "type": "string"
          }
        },
        "required": ["task_id", "user", "week", "repository", "total_contributions", "created_at"],
        "title": "IngestTaskResponse",
        "type": "object"
      },
      "IngestTaskStatus": {
        "description": "Status of a contributions ingestion task.",
        "properties": {
          "completed_at": {
            "anyOf": [
              {
                "format": "date-time",
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Completed At"
          },
          "created_at": {
            "format": "date-time",
            "title": "Created At",
            "type": "string"
          },
          "embedding_job_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Embedding Job Id"
          },
          "error_message": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Error Message"
          },
          "failed_count": {
            "title": "Failed Count",
            "type": "integer"
          },
          "ingested_count": {
            "title": "Ingested Count",
            "type": "integer"
          },
          "processing_time_ms": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Processing Time Ms"
          },
          "repository": {
            "title": "Repository",
            "type": "string"
          },
          "started_at": {
            "anyOf": [
              {
                "format": "date-time",
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Started At"
          },
          "status": {
            "$ref": "#/components/schemas/TaskStatus"
          },
          "summary": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/SummaryResponse"
              },
              {
                "type": "null"
              }
            ]
          },
          "task_id": {
            "title": "Task Id",
            "type": "string"
          },
          "total_contributions": {
            "title": "Total Contributions",
            "type": "integer"
          },
          "user": {
            "title": "User",
            "type": "string"
          },
          "week": {
            "title": "Week",
            "type": "string"
          }
        },
        "required": [
          "task_id",
          "user",
          "week",
          "repository",
          "status",
          "total_contributions",
          "ingested_count",
          "failed_count",
          "created_at"
        ],
        "title": "IngestTaskStatus",
        "type": "object"
      },
      "QuestionContext": {
        "description": "Context configuration for question answering.",
        "properties": {
          "focus_areas": {
            "items": {
              "type": "string"
            },
            "title": "Focus Areas",
            "type": "array"
          },
          "include_evidence": {
            "default": true,
            "title": "Include Evidence",
            "type": "boolean"
          },
          "max_evidence_items": {
            "default": 10,
            "title": "Max Evidence Items",
            "type": "integer"
          },
          "reasoning_depth": {
            "$ref": "#/components/schemas/ReasoningDepth",
            "default": "detailed"
          }
        },
        "title": "QuestionContext",
        "type": "object"
      },
      "QuestionEvidence": {
        "description": "Evidence supporting a question answer.",
        "properties": {
          "contribution_id": {
            "title": "Contribution Id",
            "type": "string"
          },
          "contribution_type": {
            "$ref": "#/components/schemas/ContributionType"
          },
          "excerpt": {
            "title": "Excerpt",
            "type": "string"
          },
          "relevance_score": {
            "maximum": 1.0,
            "minimum": 0.0,
            "title": "Relevance Score",
            "type": "number"
          },
          "timestamp": {
            "format": "date-time",
            "title": "Timestamp",
            "type": "string"
          },
          "title": {
            "title": "Title",
            "type": "string"
          }
        },
        "required": [
          "title",
          "contribution_id",
          "contribution_type",
          "excerpt",
          "relevance_score",
          "timestamp"
        ],
        "title": "QuestionEvidence",
        "type": "object"
      },
      "QuestionRequest": {
        "description": "Request to ask a question about a user's week.",
        "properties": {
          "context": {
            "$ref": "#/components/schemas/QuestionContext"
          },
          "github_pat": {
            "title": "Github Pat",
            "type": "string"
          },
          "question": {
            "title": "Question",
            "type": "string"
          },
          "summary": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Summary"
          }
        },
        "required": ["question", "github_pat"],
        "title": "QuestionRequest",
        "type": "object"
      },
      "QuestionResponse": {
        "description": "Response to a question about a user's week.",
        "properties": {
          "answer": {
            "title": "Answer",
            "type": "string"
          },
          "asked_at": {
            "format": "date-time",
            "title": "Asked At",
            "type": "string"
          },
          "confidence": {
            "maximum": 1.0,
            "minimum": 0.0,
            "title": "Confidence",
            "type": "number"
          },
          "conversation_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Conversation Id"
          },
          "evidence": {
            "items": {
              "$ref": "#/components/schemas/QuestionEvidence"
            },
            "title": "Evidence",
            "type": "array"
          },
          "question": {
            "title": "Question",
            "type": "string"
          },
          "question_id": {
            "title": "Question Id",
            "type": "string"
          },
          "reasoning_steps": {
            "items": {
              "type": "string"
            },
            "title": "Reasoning Steps",
            "type": "array"
          },
          "response_time_ms": {
            "title": "Response Time Ms",
            "type": "integer"
          },
          "suggested_actions": {
            "items": {
              "type": "string"
            },
            "title": "Suggested Actions",
            "type": "array"
          },
          "user": {
            "title": "User",
            "type": "string"
          },
          "week": {
            "title": "Week",
            "type": "string"
          }
        },
        "required": [
          "question_id",
          "user",
          "week",
          "question",
          "answer",
          "confidence",
          "asked_at",
          "response_time_ms"
        ],
        "title": "QuestionResponse",
        "type": "object"
      },
      "ReasoningDepth": {
        "description": "Depth levels for question answering reasoning.",
        "enum": ["quick", "detailed", "deep"],
        "title": "ReasoningDepth",
        "type": "string"
      },
      "SummaryMetadata": {
        "description": "Metadata about a generated summary.",
        "properties": {
          "commits_count": {
            "title": "Commits Count",
            "type": "integer"
          },
          "generated_at": {
            "format": "date-time",
            "title": "Generated At",
            "type": "string"
          },
          "issues_count": {
            "title": "Issues Count",
            "type": "integer"
          },
          "pull_requests_count": {
            "title": "Pull Requests Count",
            "type": "integer"
          },
          "releases_count": {
            "title": "Releases Count",
            "type": "integer"
          },
          "repositories": {
            "items": {
              "type": "string"
            },
            "title": "Repositories",
            "type": "array"
          },
          "time_period": {
            "title": "Time Period",
            "type": "string"
          },
          "total_contributions": {
            "title": "Total Contributions",
            "type": "integer"
          }
        },
        "required": [
          "total_contributions",
          "commits_count",
          "pull_requests_count",
          "issues_count",
          "releases_count",
          "repositories",
          "time_period",
          "generated_at"
        ],
        "title": "SummaryMetadata",
        "type": "object"
      },
      "SummaryResponse": {
        "description": "Complete summary response (non-streaming).",
        "properties": {
          "analysis": {
            "title": "Analysis",
            "type": "string"
          },
          "areas_for_improvement": {
            "items": {
              "type": "string"
            },
            "title": "Areas For Improvement",
            "type": "array"
          },
          "commits_summary": {
            "title": "Commits Summary",
            "type": "string"
          },
          "generated_at": {
            "format": "date-time",
            "title": "Generated At",
            "type": "string"
          },
          "issues_summary": {
            "title": "Issues Summary",
            "type": "string"
          },
          "key_achievements": {
            "items": {
              "type": "string"
            },
            "title": "Key Achievements",
            "type": "array"
          },
          "metadata": {
            "$ref": "#/components/schemas/SummaryMetadata"
          },
          "overview": {
            "title": "Overview",
            "type": "string"
          },
          "pull_requests_summary": {
            "title": "Pull Requests Summary",
            "type": "string"
          },
          "releases_summary": {
            "title": "Releases Summary",
            "type": "string"
          },
          "summary_id": {
            "title": "Summary Id",
            "type": "string"
          },
          "user": {
            "title": "User",
            "type": "string"
          },
          "week": {
            "title": "Week",
            "type": "string"
          }
        },
        "required": [
          "summary_id",
          "user",
          "week",
          "overview",
          "commits_summary",
          "pull_requests_summary",
          "issues_summary",
          "releases_summary",
          "analysis",
          "key_achievements",
          "areas_for_improvement",
          "metadata",
          "generated_at"
        ],
        "title": "SummaryResponse",
        "type": "object"
      },
      "TaskStatus": {
        "description": "Status values for asynchronous tasks.",
        "enum": ["queued", "ingesting", "summarizing", "done", "failed"],
        "title": "TaskStatus",
        "type": "string"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": ["loc", "msg", "type"],
        "title": "ValidationError",
        "type": "object"
      }
    }
  },
  "info": {
    "description": "AI-powered service for ingesting GitHub contributions and providing Q&A for user weekly summaries",
    "title": "Prompteus GenAI Service",
    "version": "1.0.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/contributions": {
      "post": {
        "description": "Start an asynchronous ingestion task for GitHub contributions.",
        "operationId": "start_contributions_ingestion_task_contributions_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ContributionsIngestRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IngestTaskResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Start Contributions Ingestion Task"
      }
    },
    "/ingest/{task_id}": {
      "get": {
        "description": "Get the status of a contributions ingestion task.",
        "operationId": "get_ingestion_task_status_ingest__task_id__get",
        "parameters": [
          {
            "description": "Task ID returned from POST /contributions",
            "in": "path",
            "name": "task_id",
            "required": true,
            "schema": {
              "description": "Task ID returned from POST /contributions",
              "title": "Task Id",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IngestTaskStatus"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get Ingestion Task Status"
      }
    },
    "/users/{username}/weeks/{week_id}/questions": {
      "post": {
        "description": "Ask a question about a user's contributions for a specific week.",
        "operationId": "ask_question_about_user_contributions_users__username__weeks__week_id__questions_post",
        "parameters": [
          {
            "description": "GitHub username",
            "in": "path",
            "name": "username",
            "required": true,
            "schema": {
              "description": "GitHub username",
              "title": "Username",
              "type": "string"
            }
          },
          {
            "description": "ISO week format: 2024-W21",
            "in": "path",
            "name": "week_id",
            "required": true,
            "schema": {
              "description": "ISO week format: 2024-W21",
              "title": "Week Id",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/QuestionRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/QuestionResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Ask Question About User Contributions"
      }
    }
  },
  "servers": [
    {
      "url": "/api/genai"
    }
  ]
}
